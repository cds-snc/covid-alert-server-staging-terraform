{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "COVID Alert Metrics Schema Version 1.4.2",
  "required": [
    "metricstimestamp",
    "appversion",
    "appos",
    "payload"
  ],
  "properties": {
    "metricstimestamp": {
      "type": "integer",
      "minLength": 0,
      "maxLength": 25,
      "title": "Metrics time stamp Schema",
      "default": "null"
    },
    "symptomonset": {
      "type": "boolean",
      "title": "Symptom on set Schema"
    },
    "appversion": {
      "type": "string",
      "minLength": 0,
      "maxLength": 5,
      "title": "App Version Schema",
      "default": "null"
    },
    "appos": {
      "type": "string",
      "minLength": 0,
      "maxLength": 20,
      "title": "App os Schema",
      "default": "null"
    },
    "payload": {
      "type": "array",
      "title": "payload Schema",
      "default": [],
      "minItems": 1,
      "items": {
        "type": "object",
        "title": "payload Schema",
        "default": {},
        "required": [
          "identifier",
          "region",
          "timestamp"
        ]
      }
    }
  }
}